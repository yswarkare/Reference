<h1>About Page</h1>


<div class="card-deck">

    {{#each team}}
    <div class="card">
        <img src={{this.imageUrl}} class="card-img-top" />
        <div class="card-body">
            <h5 class="card-title">{{this.firstname}} {{this.lastname}}</h5>
            <p class="card-text">{{this.bio}}</p>
            <p class="card-text"><small class="text-muted">Last updated 3 mins ago</small></p>
        </div>
    </div>
    {{/each}}
    
</div>